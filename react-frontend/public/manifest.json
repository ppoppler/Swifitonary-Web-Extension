{
  "manifest_version": 2,

  "name": "React Frontend",
  "description": "Frontend template for Swiftionary",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "slogo_16.png",
    "48": "slogo_48.png",
    "128": "slogo_128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'",
  "permissions": [],
  "background": {
    "scripts": ["conMenus.js"],
    "persistent": false
  }
}
