{
  "manifest_version": 2,

  "name": "React Frontend",
  "description": "Frontend template for Swiftionary",
  "version": "0.0.1",
  "content_scripts": [{
    "js": ["content.js"],
    "matches":["<all_urls>"]
  }],
  "icons": {
    "16": "slogo_16.png",
    "48": "slogo_48.png",
    "128": "slogo_128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["contextMenus", "tabs"],
  "web_accessible_resources": ["*.html"]
}
